<h1> Créer une réservation </h1>

<%= simple_form_for(@booking) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :address %>
    <%= f.input :status%>
  <%= f.button :submit %>
<% end %>
