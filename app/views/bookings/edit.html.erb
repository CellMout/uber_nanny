<h1>Edit booking</h1>

<%= simple_form_for [@nanny, @booking] do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :address %>
  </div>

  <%= f.button :submit, "Edit booking", class: 'btn btn-primary' %>
<% end %>
